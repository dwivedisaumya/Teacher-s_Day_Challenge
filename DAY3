#include<iostream>
using namespace std;
class Solution {
public:
    bool isPalindrome(string s) {
        string result;
        for (char c:s){
            if (isalnum(c)){
                result+=tolower(c);
            }
        
        }
        string rev=result;
        std::reverse(rev.begin(),rev.end());
        return rev==result;
    }
};
